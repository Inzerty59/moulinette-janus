{% extends 'base.html.twig' %}

{% block title %}Agency{% endblock %}

{% block body %}
    <h1>Agency</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ agency.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ agency.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Timezone</th>
                <td>{{ agency.timezone }}</td>
            </tr>
            <tr>
                <th>TotalsPolicy</th>
                <td>{{ agency.totalsPolicy ? agency.totalsPolicy|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Settings</th>
                <td>{{ agency.settings ? agency.settings|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ agency.createdAt ? agency.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ agency.updatedAt ? agency.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_agency_crud_index') }}">back to list</a>

    <a href="{{ path('app_agency_crud_edit', {'id': agency.id}) }}">edit</a>

    {{ include('agency_crud/_delete_form.html.twig') }}
{% endblock %}
