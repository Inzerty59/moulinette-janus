{% extends 'base.html.twig' %}

{% block title %}AgencyRubric index{% endblock %}

{% block body %}
    <h1>AgencyRubric index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Code</th>
                <th>Name</th>
                <th>Category</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for agency_rubric in agency_rubrics %}
            <tr>
                <td>{{ agency_rubric.id }}</td>
                <td>{{ agency_rubric.code }}</td>
                <td>{{ agency_rubric.name }}</td>
                <td>{{ agency_rubric.category }}</td>
                <td>{{ agency_rubric.createdAt ? agency_rubric.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ agency_rubric.updatedAt ? agency_rubric.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_agency_rubric_crud_show', {'id': agency_rubric.id}) }}">show</a>
                    <a href="{{ path('app_agency_rubric_crud_edit', {'id': agency_rubric.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_agency_rubric_crud_new') }}">Create new</a>
{% endblock %}
